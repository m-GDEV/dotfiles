Section "Monitor"
	Identifier   "HDMI1"
	VendorName   "Monitor Vendor"
	ModelName    "Monitor Model"
	Option       "Enable" "true"
	Option       "LeftOf" "HDMI1"
EndSection

Section "Monitor"
	Identifier   "HDMI2"
